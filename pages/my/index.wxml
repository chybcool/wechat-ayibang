<view class="container">
  <view class="avatar flex">
    <image src="{{images}}">
    </image>
    <text class="login" bindtap="bindViewTap">{{login}}</text>
  </view>
  <view class="cells">
    <block wx:for="{{items}}" wx:key="{{item}}">
      <view class="cell">
        <view class="cell-bd">
          <text class="desc">{{item.desc}}</text>
        </view>
        <view class="cell-ft"></view>
      </view>
    </block>
  </view>
</view>
